{
  "name": "color-compare",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://redundantrobot.com/chroma_extract",
  "dependencies": {
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "gh-pages": "^2.0.1",
    "lodash": "^4.17.21",
    "node-sass": "^9.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "4.0.3",
    "react-test-renderer": "^16.8.6",
    "styled-components": "^5.3.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "INLINE_RUNTIME_CHUNK=false GENERATE_SOURCEMAP=false REACT_APP_DEPLOY_DIRECTORY=chroma_extract yarn build-react-scripts",
    "build-react-scripts": "react-scripts --openssl-legacy-provider build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {}
}
